{
  "name": "eleventy-plugin-fluid",
  "version": "1.0.0",
  "description": "Eleventy plugin which provides common filters, shortcodes and transforms for Fluid Project websites.",
  "publishConfig": {
    "access": "public"
  },
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && eleventy",
    "lint": "fluid-lint-all",
    "start": "rimraf dist && eleventy --serve",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluid-project/eleventy-plugin-fluid.git"
  },
  "keywords": [
    "eleventy-plugin"
  ],
  "author": "OCAD University <idrc@ocadu.ca>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/fluid-project/eleventy-plugin-fluid/issues"
  },
  "homepage": "https://github.com/fluid-project/eleventy-plugin-fluid#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "ava": "4.3.3",
    "eslint-config-fluid": "2.0.1",
    "fluid-lint-all": "1.1.5",
    "husky": "4.3.8",
    "infusion": "4.0.0",
    "rimraf": "3.0.2"
  },
  "dependencies": {
    "html-minifier": "4.0.0"
  },
  "peerDependencies": {
    "infusion": "4.x"
  }
}
